html,body,#root,.App {
  height: 100%;
}
.App{
  display: flex;
  flex-direction: row;
}
.App .nav {
  float:left;
  width:200px;
  background-color: teal;
  height: 100%;
}
.App .nav a {
  color: #ffffff;
  text-decoration: none;
}
.App .content {
  /* margin-left: 200px; */
  flex:1;
  padding:1em;
}
.nav > ul{
  text-align: center;
  font-size: 1.5em;
}
ul > li{
  margin: 30px 0px;
}
div > h2{
  text-align: center;
}
.ant-table-body{
  margin-top: 0;
}